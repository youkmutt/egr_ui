{
  "name": "egr_ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-api": "rimraf ./src/api && openapi-generator-cli generate -i http://localhost:44317/swagger/v1/swagger.json -g typescript-fetch -o ./src/api --skip-validate-spec --additional-properties=modelPropertyNaming=original,supportsES6=true,withInterfaces=true,modelSuffix=Dto,withoutRuntimeChecks=true,skipObjectModel=true"
  },
  "dependencies": {
    "antd": "^6.1.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "rimraf": "^6.1.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}